        "terminal.ansi.dim_cyan": null,

        "terminal.ansi.white": "#d3c6aa",
        "terminal.ansi.bright_white": "#d3c6aa",
        "terminal.ansi.dim_white": null,

        "link_text.hover": "#a7c080c0",

        "conflict": "#d699b6a0",
        "conflict.background": null,
        "conflict.border": null,

        "created": "#dbbc7f",
        "created.background": "#dbbc7f33",
        "created.border": "#dbbc7f80",

        "deleted": "#da6362a0",
        "deleted.background": null,
        "deleted.border": null,

        "error": "#da6362",
        "error.background": "#4c3743",
        "error.border": null,

        "hidden": "#7f897d",
        "hidden.background": null,
        "hidden.border": null,

        "hint": "#7f897da0",
        "hint.background": null,
        "hint.border": null,

        "ignored": "#495156",
        "ignored.background": null,
        "ignored.border": null,

        "info": "#7fbbb3",
        "info.background": "#7fbbb333",
        "info.border": "#7fbbb380",

        "modified": "#7fbbb3",
        "modified.background": "#7fbbb333",
        "modified.border": "#7fbbb380",

        "predictive": "#7f897d",
        "predictive.background": null,
        "predictive.border": null,

        "renamed": null,
        "renamed.background": null,
        "renamed.border": null,

        "success": null,
        "success.background": null,
        "success.border": null,

        "unreachable": null,
        "unreachable.background": null,
        "unreachable.border": null,

        "warning": "#bf983d",
        "warning.background": "#4D4C43",
        "warning.border": null,

        "players": [
          {
            "cursor": "#d3c6aa",
            "background": "#223040",
            "selection": "#223040"
          }
        ],

        "syntax": {
          "attribute": { "color": "#dbbc7f" },
          "boolean": { "color": "#d699b6" },
          "comment": { "color": "#859289", "font_style": "italic" },
          "comment.doc": { "color": "#859289", "font_style": "italic" },
          "constant": { "color": "#d699b6" },
          "constructor": { "color": "#d699b6" },
          "function": { "color": "#a7c080" },
          "keyword": { "color": "#e67e80" },
          "label": { "color": "#d3c6aa" },
          "number": { "color": "#d699b6" },
          "operator": { "color": "#e69875" },
          "preproc": { "color": "#e67e80" },
          "punctuation": { "color": "#9aa79d" },
          "punctuation.bracket": { "color": "#9aa79d" },
          "punctuation.delimiter": { "color": "#9aa79d" },
          "property": { "color": "#83c092" },
          "string": { "color": "#dbbc7f" },
          "string.escape": { "color": "#98c379" },
          "string.regex": { "color": "#dbbc7f" },
          "string.special": { "color": "#dbbc7f" },
          "tag": { "color": "#e69875" },
          "type": { "color": "#7fbbb3" },
          "variable": { "color": "#d3c6aa" },
          "variable.special": { "color": "#83c092" }
        }
