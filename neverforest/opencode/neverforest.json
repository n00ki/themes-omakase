{
  "$schema": "https://opencode.ai/theme.json",
  "defs": {
    "bg_main": "#0f1419",
    "bg_panel": "#0c1116",
    "bg_element": "#1a2330",
    "fg_text": "#d3c6aa",
    "fg_muted": "#7f897d",
    "border_main": "#16212a",
    "border_focus": "#273746",
    "forest_green": "#a7c080",
    "forest_red": "#e67e80",
    "forest_yellow": "#dbbc7f",
    "forest_blue": "#7fbbb3",
    "forest_magenta": "#d699b6",
    "forest_cyan": "#83c092",
    "forest_orange": "#e69875",
    "forest_warn": "#bf983d",
    "diff_add_bg": "#a7c08020",
    "diff_rem_bg": "#e67e8020"
  },
  "theme": {
    "primary": { "dark": "forest_green", "light": "forest_green" },
    "secondary": { "dark": "forest_blue", "light": "forest_blue" },
    "accent": { "dark": "forest_yellow", "light": "forest_yellow" },
    "error": { "dark": "forest_red", "light": "forest_red" },
    "warning": { "dark": "forest_warn", "light": "forest_warn" },
    "success": { "dark": "forest_green", "light": "forest_green" },
    "info": { "dark": "forest_blue", "light": "forest_blue" },
    "text": { "dark": "fg_text", "light": "fg_text" },
    "textMuted": { "dark": "fg_muted", "light": "fg_muted" },
    "background": { "dark": "bg_main", "light": "bg_main" },
    "backgroundPanel": { "dark": "bg_panel", "light": "bg_panel" },
    "backgroundElement": { "dark": "bg_element", "light": "bg_element" },
    "border": { "dark": "border_main", "light": "border_main" },
    "borderActive": { "dark": "border_focus", "light": "border_focus" },
    "borderSubtle": { "dark": "border_main", "light": "border_main" },
    "diffAdded": { "dark": "forest_green", "light": "forest_green" },
    "diffRemoved": { "dark": "forest_red", "light": "forest_red" },
    "diffContext": { "dark": "fg_muted", "light": "fg_muted" },
    "diffHunkHeader": { "dark": "forest_blue", "light": "forest_blue" },
    "diffHighlightAdded": { "dark": "forest_green", "light": "forest_green" },
    "diffHighlightRemoved": { "dark": "forest_red", "light": "forest_red" },
    "diffAddedBg": { "dark": "diff_add_bg", "light": "diff_add_bg" },
    "diffRemovedBg": { "dark": "diff_rem_bg", "light": "diff_rem_bg" },
    "diffContextBg": { "dark": "bg_panel", "light": "bg_panel" },
    "diffLineNumber": { "dark": "fg_muted", "light": "fg_muted" },
    "diffAddedLineNumberBg": { "dark": "diff_add_bg", "light": "diff_add_bg" },
    "diffRemovedLineNumberBg": {
      "dark": "diff_rem_bg",
      "light": "diff_rem_bg"
    },
    "markdownText": { "dark": "fg_text", "light": "fg_text" },
    "markdownHeading": { "dark": "forest_green", "light": "forest_green" },
    "markdownLink": { "dark": "forest_blue", "light": "forest_blue" },
    "markdownLinkText": { "dark": "forest_cyan", "light": "forest_cyan" },
    "markdownCode": { "dark": "forest_green", "light": "forest_green" },
    "markdownBlockQuote": { "dark": "fg_muted", "light": "fg_muted" },
    "markdownEmph": { "dark": "forest_yellow", "light": "forest_yellow" },
    "markdownStrong": { "dark": "forest_orange", "light": "forest_orange" },
    "markdownHorizontalRule": { "dark": "border_main", "light": "border_main" },
    "markdownListItem": { "dark": "fg_text", "light": "fg_text" },
    "markdownListEnumeration": {
      "dark": "forest_green",
      "light": "forest_green"
    },
    "markdownImage": { "dark": "forest_magenta", "light": "forest_magenta" },
    "markdownImageText": { "dark": "forest_yellow", "light": "forest_yellow" },
    "markdownCodeBlock": { "dark": "fg_text", "light": "fg_text" },
    "syntaxComment": { "dark": "fg_muted", "light": "fg_muted" },
    "syntaxKeyword": { "dark": "forest_red", "light": "forest_red" },
    "syntaxFunction": { "dark": "forest_green", "light": "forest_green" },
    "syntaxVariable": { "dark": "fg_text", "light": "fg_text" },
    "syntaxString": { "dark": "forest_yellow", "light": "forest_yellow" },
    "syntaxNumber": { "dark": "forest_magenta", "light": "forest_magenta" },
    "syntaxType": { "dark": "forest_blue", "light": "forest_blue" },
    "syntaxOperator": { "dark": "forest_orange", "light": "forest_orange" },
    "syntaxPunctuation": { "dark": "fg_text", "light": "fg_text" }
  }
}
