        "tab_bar.background": "#0f1016",
        "tab.inactive_background": "#0f1016",
        "tab.active_background": "#141520",

        "search.match_background": "#2d4f6766",

        "panel.background": "#0f1016",
        "panel.focused_border": "#8ba4b0",
        "pane.focused_border": "#8ba4b0",

        "scrollbar.thumb.background": "#1b273966",
