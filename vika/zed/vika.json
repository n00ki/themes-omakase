{
  "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
  "name": "Vika",
  "author": "Noam Shemesh",
  "description": "A grounded, earthy utilitarian dark theme",
  "themes": [
    {
      "name": "Vika",
      "appearance": "dark",
      "style": {
        "border": "#0f111d",
        "border.variant": "#11141e",
        "border.focused": "#8ba4b0",
        "border.selected": "#0f111d",
        "border.transparent": "#00000000",
        "border.disabled": "#0f111d",

        "background": "#0f1016",
