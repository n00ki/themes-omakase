        "elevated_surface.background": "#11141e",

        "element.background": "#11141e",
        "element.hover": "#181a2a",
        "element.active": "#223249",
        "element.selected": "#181a2a",
        "element.disabled": "#575653",

        "drop_target.background": "#22324966",
        "ghost_element.background": null,
        "ghost_element.hover": "#181a2a",
