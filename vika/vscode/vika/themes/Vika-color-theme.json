  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#737c73"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid", "invalid.illegal"],
      "settings": { "foreground": "#c4746e" }
    },
    {
      "name": "Keywords and Storage",
      "scope": ["keyword", "storage", "storage.type", "storage.modifier"],
      "settings": { "foreground": "#8992a7" }
    },
    {
      "name": "Operators",
      "scope": [
        "keyword.operator",
        "keyword.operator.assignment",
        "keyword.operator.arithmetic",
        "keyword.operator.comparison",
        "keyword.operator.logical"
      ],
      "settings": { "foreground": "#c4b28a" }
    },
    {
      "name": "Optional indicator and optional chaining",
      "scope": [
        "keyword.operator.optional",
        "punctuation.definition.optional",
        "punctuation.accessor.optional",
        "punctuation.separator.optional"
      ],
      "settings": { "foreground": "#9e9b93" }
    },
    {
      "name": "Punctuation and Brackets",
      "scope": [
        "punctuation",
        "punctuation.definition.tag",
        "punctuation.separator",
        "punctuation.terminator",
        "punctuation.section",
        "punctuation.definition.arguments",
        "punctuation.definition.parameters",
        "punctuation.definition.array",
        "punctuation.definition.list",
        "punctuation.definition.template-expression",
        "punctuation.definition.string",
        "punctuation.definition.variable",
        "punctuation.section.braces",
        "punctuation.section.brackets",
        "punctuation.section.parens",
        "meta.brace",
        "meta.bracket"
      ],
      "settings": { "foreground": "#9e9b93" }
    },
    {
      "name": "Tags",
      "scope": ["entity.name.tag", "meta.tag", "meta.tag.sgml"],
      "settings": { "foreground": "#c4b28a" }
    },
    {
      "name": "Attributes",
      "scope": [
        "entity.other.attribute-name",
        "meta.attribute",
        "meta.attribute-with-value",
        "entity.other.attribute-name.id",
        "entity.other.attribute-name.class"
      ],
      "settings": { "foreground": "#8992a7" }
    },
    {
      "name": "Functions and Methods",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "variable.function",
        "support.function",
        "entity.name.method",
        "meta.method-call"
      ],
      "settings": { "foreground": "#8ba4b0" }
    },
    {
      "name": "Constructors",
      "scope": ["entity.name.type.constructor", "support.function.constructor"],
      "settings": { "foreground": "#8ea4a2" }
    },
    {
      "name": "Classes, Types, Namespaces, and Modules",
      "scope": [
        "entity.name.type",
        "entity.name.class",
        "entity.name.namespace",
        "support.class",
        "support.type",
        "support.other.namespace",
        "support.type.sys-types",
        "entity.name.type.module",
        "keyword.other.type"
      ],
      "settings": { "foreground": "#a0b29f" }
    },
    {
      "name": "Variables (non-parameters/properties)",
      "scope": ["variable", "variable.other"],
      "settings": { "foreground": "#c4b28a" }
    },
    {
      "name": "Import/Export Variables",
      "scope": ["variable.other.readwrite"],
      "settings": { "foreground": "#a0b29f" }
    },
    {
      "name": "Parameters and Properties",
      "scope": [
        "variable.parameter",
        "meta.parameter",
        "variable.other.parameter",
        "variable.other.property",
        "variable.other.member"
      ],
      "settings": { "foreground": "#c5c9c5" }
    },
    {
      "name": "Special Variables (this, super, etc.)",
      "scope": ["variable.language", "variable.other.readwrite.this"],
      "settings": { "foreground": "#8ea4a2", "fontStyle": "italic" }
    },
    {
      "name": "Constants",
      "scope": [
        "constant",
        "support.constant",
        "constant.support",
        "constant.character",
        "constant.escape"
      ],
      "settings": { "foreground": "#c5c9c5" }
    },
    {
      "name": "Constants (language)",
      "scope": ["constant.language", "constant.language.boolean"],
      "settings": { "foreground": "#b6927b" }
    },
    {
      "name": "Numbers",
      "scope": ["constant.numeric"],
      "settings": { "foreground": "#a292a3" }
    },
    {
      "name": "Strings",
      "scope": [
        "string",
        "constant.other.symbol",
        "constant.other.key",
        "meta.group.braces.curly constant.other.object.key string.unquoted.label"
      ],
      "settings": { "foreground": "#ad8a94" }
    },
    {
      "name": "String escape sequences",
      "scope": ["constant.character.escape", "string.escape"],
      "settings": { "foreground": "#949fb5" }
    },
    {
      "name": "Regular Expressions",
      "scope": ["string.regexp", "string.regex"],
      "settings": { "foreground": "#b98d7b" }
    },
    {
      "name": "Regex Subcomponents",
      "scope": [
        "string.regexp.character-class",
        "string.regexp.constant.character.escape",
        "string.regexp.arbitrary-repetition"
      ],
      "settings": { "foreground": "#b98d7b" }
    },
    {
      "name": "Preprocessor, Macro",
      "scope": ["meta.preprocessor", "entity.name.function.preprocessor"],
      "settings": { "foreground": "#b6927b" }
    },
    {
      "name": "Decorators/Annotations",
      "scope": ["meta.decorator", "storage.type.annotation", "meta.annotation"],
      "settings": { "foreground": "#8ba4b0", "fontStyle": "italic" }
    },
    {
      "name": "Markup: headings",
      "scope": [
        "markup.heading",
        "markup.heading entity.name",
        "markup.heading.markdown punctuation.definition.heading.markdown"
      ],
      "settings": { "foreground": "#a0b29f" }
    },
    {
      "name": "Markup: bold",
      "scope": ["markup.bold", "markup.bold string"],
      "settings": { "fontStyle": "bold", "foreground": "#c5c9c5" }
    },
    {
      "name": "Markup: italic",
      "scope": ["markup.italic"],
      "settings": { "fontStyle": "italic", "foreground": "#c5c9c5" }
    },
    {
      "name": "Markup: underline",
      "scope": ["markup.underline"],
      "settings": { "fontStyle": "underline", "foreground": "#8ba4b0" }
    },
    {
      "name": "Markup: quotes and tables",
      "scope": ["markup.quote", "markup.table"],
      "settings": { "foreground": "#c5c9c5" }
    },
    {
      "name": "Links",
      "scope": [
        "*url*",
        "*link*",
        "*uri*",
        "string.other.link",
        "string.other.link.title.markdown"
      ],
      "settings": { "foreground": "#8ba4b0", "fontStyle": "underline" }
    },
    {
      "name": "Inserted (diff/markup)",
      "scope": ["markup.inserted"],
