{
  "$schema": "https://opencode.ai/theme.json",
  "defs": {
    "bg_main": "#0f1016",
    "bg_panel": "#11141e",
    "bg_element": "#181a2a",
    "fg_text": "#c5c9c5",
    "fg_muted": "#737c73",
    "border_main": "#0f111d",
    "border_focus": "#8ba4b0",
    "accent_blue": "#8ba4b0",
    "accent_cyan": "#8ea4a2",
    "accent_green": "#a0b29f",
    "accent_yellow": "#c4b28a",
    "accent_red": "#c4746e",
    "accent_magenta": "#ad8a94",
    "warning_orange": "#d4976c",
    "diff_add_bg": "#22324966",
    "diff_rem_bg": "#c4746e33"
  },
  "theme": {
    "primary": { "dark": "accent_blue", "light": "accent_blue" },
    "secondary": { "dark": "accent_cyan", "light": "accent_cyan" },
    "accent": { "dark": "accent_yellow", "light": "accent_yellow" },
    "error": { "dark": "accent_red", "light": "accent_red" },
    "warning": { "dark": "warning_orange", "light": "warning_orange" },
    "success": { "dark": "accent_green", "light": "accent_green" },
    "info": { "dark": "accent_blue", "light": "accent_blue" },
    "text": { "dark": "fg_text", "light": "fg_text" },
    "textMuted": { "dark": "fg_muted", "light": "fg_muted" },
    "background": { "dark": "bg_main", "light": "bg_main" },
    "backgroundPanel": { "dark": "bg_panel", "light": "bg_panel" },
    "backgroundElement": { "dark": "bg_element", "light": "bg_element" },
    "border": { "dark": "border_main", "light": "border_main" },
    "borderActive": { "dark": "border_focus", "light": "border_focus" },
    "borderSubtle": { "dark": "border_main", "light": "border_main" },
    "diffAdded": { "dark": "accent_green", "light": "accent_green" },
    "diffRemoved": { "dark": "accent_red", "light": "accent_red" },
    "diffContext": { "dark": "fg_muted", "light": "fg_muted" },
    "diffHunkHeader": { "dark": "accent_blue", "light": "accent_blue" },
    "diffHighlightAdded": { "dark": "accent_green", "light": "accent_green" },
    "diffHighlightRemoved": { "dark": "accent_red", "light": "accent_red" },
    "diffAddedBg": { "dark": "diff_add_bg", "light": "diff_add_bg" },
    "diffRemovedBg": { "dark": "diff_rem_bg", "light": "diff_rem_bg" },
    "diffContextBg": { "dark": "bg_panel", "light": "bg_panel" },
    "diffLineNumber": { "dark": "fg_muted", "light": "fg_muted" },
    "diffAddedLineNumberBg": { "dark": "diff_add_bg", "light": "diff_add_bg" },
    "diffRemovedLineNumberBg": {
      "dark": "diff_rem_bg",
      "light": "diff_rem_bg"
    },
    "markdownText": { "dark": "fg_text", "light": "fg_text" },
    "markdownHeading": { "dark": "accent_blue", "light": "accent_blue" },
    "markdownLink": { "dark": "accent_cyan", "light": "accent_cyan" },
    "markdownLinkText": { "dark": "accent_blue", "light": "accent_blue" },
    "markdownCode": { "dark": "accent_green", "light": "accent_green" },
    "markdownBlockQuote": { "dark": "fg_muted", "light": "fg_muted" },
    "markdownEmph": { "dark": "accent_yellow", "light": "accent_yellow" },
    "markdownStrong": { "dark": "accent_yellow", "light": "accent_yellow" },
    "markdownHorizontalRule": { "dark": "border_main", "light": "border_main" },
    "markdownListItem": { "dark": "fg_text", "light": "fg_text" },
    "markdownListEnumeration": {
      "dark": "accent_blue",
      "light": "accent_blue"
    },
    "markdownImage": { "dark": "accent_magenta", "light": "accent_magenta" },
    "markdownImageText": { "dark": "accent_cyan", "light": "accent_cyan" },
    "markdownCodeBlock": { "dark": "fg_text", "light": "fg_text" },
    "syntaxComment": { "dark": "fg_muted", "light": "fg_muted" },
    "syntaxKeyword": { "dark": "accent_blue", "light": "accent_blue" },
    "syntaxFunction": { "dark": "accent_blue", "light": "accent_blue" },
    "syntaxVariable": { "dark": "accent_yellow", "light": "accent_yellow" },
    "syntaxString": { "dark": "accent_magenta", "light": "accent_magenta" },
    "syntaxNumber": { "dark": "accent_magenta", "light": "accent_magenta" },
    "syntaxType": { "dark": "accent_green", "light": "accent_green" },
    "syntaxOperator": { "dark": "accent_yellow", "light": "accent_yellow" },
    "syntaxPunctuation": { "dark": "fg_muted", "light": "fg_muted" }
  }
}
